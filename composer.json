{
    "name":"mooti/validator",
    "description": "PHP validator for json style data structures",
    "keywords": ["validation", "validator", "php"],
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.0.0",
        "mooti/factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.* || 5.6.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {"Mooti\\Validator\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Mooti\\Test\\PHPUnit\\Validator\\": "tests/phpunit/src"}
    }
}
